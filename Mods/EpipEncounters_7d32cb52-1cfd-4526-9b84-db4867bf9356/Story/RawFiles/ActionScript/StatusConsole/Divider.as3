package
{
    import flash.display.MovieClip;
    
    public dynamic class Divider extends MovieClip
    {
        public function Divider()
        {
            super();
            this.stop();
        }

        public function setSpecialSprite(enabled:Boolean) : *
        {
            this.gotoAndStop(enabled ? 2 : 1);

            this.y = enabled ? -10 : 0;
        }
    }
}